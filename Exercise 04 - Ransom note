function setup() {
  createCanvas(800, 200);
  background(255);
  let textToDisplay = "Give me 100K";
  let x = 10; 
  for (let i = 0; i < textToDisplay.length; i++) {
    let fontSize = random(24, 48); 
    textSize(fontSize);
    text(textToDisplay[i], x, 100); 
    x += textWidth(textToDisplay[i]) + 5; 
  }
}
